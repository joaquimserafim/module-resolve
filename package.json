    "installed",
    "util",
    "utilities"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/module-resolve/issues"
  },
  "homepage": "https://github.com/joaquimserafim/module-resolve",
  "devDependencies": {
    "code": "^1.3.0",
    "jscs": "^1.11.3",
    "jshint": "^2.6.0",
    "lab": "^5.2.1",
    "pre-commit": "^1.0.4"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
